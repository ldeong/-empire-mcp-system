{
  "name": "sina-empire-mcp-system",
  "version": "1.0.0",
  "type": "module",
  "description": "Comprehensive MCP ecosystem for unlimited scaling of Claude API calls into Cloudflare Workers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-mcp-ecosystem.js",
    "test:unit": "jest",
    "setup": "node scripts/setup.js",
    "lint": "eslint *.js scripts/*.js",
    "format": "prettier --write *.js scripts/*.js",
    "master": "node master-agent.js start",
    "master:stop": "node master-agent.js stop",
    "master:restart": "node master-agent.js restart",
    "master:status": "node master-agent.js status",
    "master:deploy": "node master-agent.js deploy",
    "master:income": "node master-agent.js income",
    "master:diagnose": "node master-agent.js diagnose",
    "empire": "node master-agent.js start",
    "empire:income": "node master-agent.js income",
    "empire:deploy": "node master-agent.js deploy",
    "empire:diagnose": "node master-agent.js diagnose",
    "hive": "node sina-hive-mind.js start",
    "hive:test": "node sina-hive-mind.js test",
    "money": "node sina-hive-mind.js start",
    "make-money": "curl -X POST http://localhost:3007/voice/command -H 'Content-Type: application/json' -d '{\"command\": \"make money now\", \"sessionId\": \"cli\"}'",
    "blitz": "curl -X POST http://localhost:3007/voice/command -H 'Content-Type: application/json' -d '{\"command\": \"activate blitz\", \"sessionId\": \"cli\"}'",
    "cf": "node cloudflare-empire-manager.js start",
    "cf:deploy": "node cloudflare-empire-manager.js deploy",
    "cf:scan": "node cloudflare-empire-manager.js scan",
    "cf:build": "node cloudflare-empire-manager.js build",
    "deploy:world": "node cloudflare-empire-manager.js deploy",
    "wrangler:deploy": "wrangler deploy",
    "wrangler:dev": "wrangler dev",
    "wrangler:tail": "wrangler tail",
    "empire:global": "npm run cf:deploy && npm run wrangler:deploy",
    "empire:status": "node global-empire-monitor.js",
    "empire:monitor": "watch -n 30 'node global-empire-monitor.js'",
    "empire:live": "curl -s https://sina-empire-crypto-gateway.louiewong4.workers.dev/status",
    "first-dollar": "./deploy-first-dollar.sh",
    "first-dollar:deploy": "./deploy-first-dollar.sh",
    "first-dollar:test": "node test-first-dollar.js",
    "first-dollar:status": "curl -s https://first-dollar-mission.YOUR.workers.dev/api/status || echo 'Worker not deployed yet. Run: npm run first-dollar:deploy'"
  },
  "keywords": [
    "mcp",
    "claude",
    "cloudflare",
    "api",
    "scaling",
    "resilience",
    "orchestration"
  ],
  "author": "SINA Empire",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.3",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "hono": "^4.9.8",
    "joi": "^17.11.0",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sina-empire/mcp-system.git"
  },
  "bugs": {
    "url": "https://github.com/sina-empire/mcp-system/issues"
  },
  "homepage": "https://github.com/sina-empire/mcp-system#readme"
}
